{
  "name": "@mobile-agent/sdk",
  "version": "1.0.0",
  "description": "Natural language mobile app testing SDK for Appium/WebDriverIO",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mobile-agent-mcp": "./bin/mcp-server.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "biome check .",
    "format": "biome format --write .",
    "bench:wait": "TS_NODE_TRANSPILE_ONLY=1 npx ts-node examples/benchmarks/wait-bench.ts",
    "prepublishOnly": "npm run build",
    "mcp-server": "node ./bin/mcp-server.js",
    "prepare": "husky install",
    "bench:android": "TS_NODE_TRANSPILE_ONLY=1 npx ts-node examples/benchmarks/android-settings-bench.ts",
    "bench:rn": "TS_NODE_TRANSPILE_ONLY=1 npx ts-node examples/benchmarks/sample-rn-app-bench.ts"
  },
  "keywords": [
    "mobile",
    "testing",
    "appium",
    "webdriverio",
    "ai",
    "gpt",
    "automation",
    "mcp",
    "model-context-protocol",
    "agent",
    "claude",
    "cursor",
    "vision",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.14.0",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "openai": "^4.24.0",
    "sharp": "^0.33.0",
    "webdriverio": "^8.27.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/sharp": "^0.32.0",
    "@types/xml2js": "^0.4.14",
    "@wdio/types": "^8.27.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.1.0"
  },
  "packageManager": "bun@1.1.34"
}
